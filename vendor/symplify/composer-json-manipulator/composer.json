{
    "name": "symplify\/composer-json-manipulator",
    "description": "Package to load, merge and save composer.json file(s)",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.0",
        "nette\/utils": "^3.2",
        "symfony\/config": "^5.4|^6.0",
        "symfony\/dependency-injection": "^5.4|^6.0",
        "symfony\/filesystem": "^5.4|^6.0",
        "symplify\/package-builder": "dev-main",
        "symplify\/symplify-kernel": "dev-main",
        "symplify\/smart-file-system": "dev-main"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ECSPrefix20220120\\Symplify\\ComposerJsonManipulator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ECSPrefix20220120\\Symplify\\ComposerJsonManipulator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.1-dev"
        }
    },
    "conflict": {
        "symplify\/astral": "<10.0.13",
        "symplify\/easy-coding-standard": "<10.0.13",
        "symplify\/phpstan-rules": "<10.0.13",
        "symplify\/easy-testing": "<10.0.13",
        "symplify\/rule-doc-generator-contracts": "<10.0.13",
        "symplify\/symfony-php-config": "<10.0.13",
        "symplify\/simple-php-doc-parser": "<10.0.13",
        "symplify\/php-config-printer": "<10.0.13",
        "symplify\/console-color-diff": "<10.0.13",
        "symplify\/autowire-array-parameter": "<10.0.13",
        "symplify\/markdown-diff": "<10.0.13",
        "symplify\/amnesia": "<10.0.13",
        "symplify\/phpstan-extensions": "<10.0.13",
        "symplify\/rule-doc-generator": "<10.0.13",
        "symplify\/vendor-patches": "<10.0.13",
        "symplify\/skipper": "<10.0.13",
        "symplify\/symfony-static-dumper": "<10.0.13",
        "symplify\/git-wrapper": "<10.0.13",
        "symplify\/symplify-kernel": "<9.4.70",
        "symplify\/monorepo-builder": "<10.0.13",
        "symplify\/config-transformer": "<10.0.13",
        "symplify\/easy-ci": "<10.0.13",
        "symplify\/coding-standard": "<10.0.13",
        "symplify\/latte-phpstan-compiler": "<10.0.13",
        "symplify\/template-phpstan-compiler": "<10.0.13",
        "symplify\/phpstan-latte-rules": "<10.0.13",
        "symplify\/easy-parallel": "<10.0.13",
        "symplify\/neon-config-dumper": "<10.0.13"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}