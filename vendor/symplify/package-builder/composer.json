{
    "name": "symplify\/package-builder",
    "description": "Dependency Injection, Console and Kernel toolkit for Symplify packages.",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette\/utils": "^3.2",
        "symfony\/config": "^5.4|^6.0",
        "symfony\/console": "^5.4|^6.0",
        "symfony\/dependency-injection": "^5.4|^6.0",
        "symfony\/finder": "^5.4|^6.0",
        "symplify\/symplify-kernel": "dev-main",
        "symplify\/easy-testing": "dev-main",
        "nette\/neon": "^3.3.2"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ECSPrefix20211223\\Symplify\\PackageBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ECSPrefix20211223\\Symplify\\PackageBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.1-dev"
        }
    },
    "conflict": {
        "symplify\/astral": "<10.0.5",
        "symplify\/composer-json-manipulator": "<10.0.5",
        "symplify\/easy-coding-standard": "<10.0.5",
        "symplify\/phpstan-rules": "<10.0.5",
        "symplify\/rule-doc-generator-contracts": "<10.0.5",
        "symplify\/symfony-php-config": "<10.0.5",
        "symplify\/simple-php-doc-parser": "<10.0.5",
        "symplify\/php-config-printer": "<10.0.5",
        "symplify\/console-color-diff": "<10.0.5",
        "symplify\/autowire-array-parameter": "<10.0.5",
        "symplify\/markdown-diff": "<10.0.5",
        "symplify\/amnesia": "<10.0.5",
        "symplify\/phpstan-extensions": "<10.0.5",
        "symplify\/rule-doc-generator": "<10.0.5",
        "symplify\/vendor-patches": "<10.0.5",
        "symplify\/skipper": "<10.0.5",
        "symplify\/smart-file-system": "<10.0.5",
        "symplify\/symfony-static-dumper": "<10.0.5",
        "symplify\/git-wrapper": "<10.0.5",
        "symplify\/monorepo-builder": "<10.0.5",
        "symplify\/config-transformer": "<10.0.5",
        "symplify\/easy-ci": "<10.0.5",
        "symplify\/coding-standard": "<10.0.5",
        "symplify\/latte-phpstan-compiler": "<10.0.5",
        "symplify\/template-phpstan-compiler": "<10.0.5",
        "symplify\/phpstan-latte-rules": "<10.0.5",
        "symplify\/easy-parallel": "<10.0.5",
        "symplify\/neon-config-dumper": "<10.0.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}