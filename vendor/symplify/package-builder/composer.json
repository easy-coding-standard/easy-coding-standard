{
    "name": "symplify\/package-builder",
    "description": "Dependency Injection, Console and Kernel toolkit for Symplify packages.",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette\/utils": "^3.2",
        "symfony\/config": "^5.3|^6.0",
        "symfony\/console": "^5.3|^6.0",
        "symfony\/dependency-injection": "^5.3|^6.0",
        "symfony\/finder": "^5.3|^6.0",
        "symfony\/http-kernel": "^5.3|^6.0",
        "symplify\/symplify-kernel": "dev-main",
        "symplify\/easy-testing": "dev-main",
        "nette\/neon": "^3.2"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ECSPrefix20210830\\Symplify\\PackageBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ECSPrefix20210830\\Symplify\\PackageBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "conflict": {
        "symplify\/astral": "<9.4.46",
        "symplify\/composer-json-manipulator": "<9.4.46",
        "symplify\/easy-coding-standard": "<9.4.46",
        "symplify\/phpstan-rules": "<9.4.46",
        "symplify\/psr4-switcher": "<9.4.46",
        "symplify\/rule-doc-generator-contracts": "<9.4.46",
        "symplify\/symfony-php-config": "<9.4.46",
        "symplify\/simple-php-doc-parser": "<9.4.46",
        "symplify\/php-config-printer": "<9.4.46",
        "symplify\/console-color-diff": "<9.4.46",
        "symplify\/autowire-array-parameter": "<9.4.46",
        "symplify\/markdown-diff": "<9.4.46",
        "symplify\/amnesia": "<9.4.46",
        "symplify\/phpstan-extensions": "<9.4.46",
        "symplify\/rule-doc-generator": "<9.4.46",
        "symplify\/console-package-builder": "<9.4.46",
        "symplify\/vendor-patches": "<9.4.46",
        "symplify\/skipper": "<9.4.46",
        "symplify\/smart-file-system": "<9.4.46",
        "symplify\/phpunit-upgrader": "<9.4.46",
        "symplify\/easy-hydrator": "<9.4.46",
        "symplify\/symfony-static-dumper": "<9.4.46",
        "symplify\/git-wrapper": "<9.4.46",
        "symplify\/monorepo-builder": "<9.4.46",
        "symplify\/config-transformer": "<9.4.46",
        "symplify\/easy-ci": "<9.4.46",
        "symplify\/coding-standard": "<9.4.46"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}