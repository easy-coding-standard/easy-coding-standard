{
    "name": "symplify\/easy-parallel",
    "description": "Helper package for easier CLI project parallelization",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "clue\/ndjson-react": "^1.2",
        "react\/child-process": "^0.6.3",
        "react\/event-loop": "^1.2",
        "react\/socket": "^1.9",
        "symfony\/console": "^5.3|^6.0",
        "symplify\/package-builder": "dev-main"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ECSPrefix20211123\\Symplify\\EasyParallel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ECSPrefix20211123\\Symplify\\EasyParallel\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.0-dev"
        }
    },
    "config": {
        "platform-check": false
    },
    "conflict": {
        "symplify\/astral": "<10.0.0-beta10",
        "symplify\/easy-coding-standard": "<10.0.0-beta10",
        "symplify\/phpstan-rules": "<10.0.0-beta10",
        "symplify\/easy-testing": "<10.0.0-beta10",
        "symplify\/rule-doc-generator-contracts": "<10.0.0-beta10",
        "symplify\/symfony-php-config": "<10.0.0-beta10",
        "symplify\/simple-php-doc-parser": "<10.0.0-beta10",
        "symplify\/php-config-printer": "<10.0.0-beta10",
        "symplify\/autowire-array-parameter": "<10.0.0-beta10",
        "symplify\/markdown-diff": "<10.0.0-beta10",
        "symplify\/amnesia": "<10.0.0-beta10",
        "symplify\/phpstan-extensions": "<10.0.0-beta10",
        "symplify\/rule-doc-generator": "<10.0.0-beta10",
        "symplify\/vendor-patches": "<10.0.0-beta10",
        "symplify\/skipper": "<10.0.0-beta10",
        "symplify\/symfony-static-dumper": "<10.0.0-beta10",
        "symplify\/git-wrapper": "<10.0.0-beta10",
        "symplify\/config-transformer": "<10.0.0-beta10",
        "symplify\/easy-ci": "<10.0.0-beta10",
        "symplify\/coding-standard": "<10.0.0-beta10",
        "symplify\/twig-phpstan-compiler": "<10.0.0-beta10",
        "symplify\/latte-phpstan-compiler": "<10.0.0-beta10",
        "symplify\/template-phpstan-compiler": "<10.0.0-beta10",
        "symplify\/phpstan-latte-rules": "<10.0.0-beta10",
        "symplify\/phpstan-twig-rules": "<10.0.0-beta10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}