{
    "name": "evenement\/evenement",
    "description": "\u00c9v\u00e9nement is a very simple event dispatching library for PHP",
    "keywords": [
        "event-dispatcher",
        "event-emitter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit\/phpunit": "^9 || ^6"
    },
    "autoload": {
        "psr-4": {
            "ECSPrefix202408\\Evenement\\": "src\/"
        }
    },
    "autoload-dev": {
